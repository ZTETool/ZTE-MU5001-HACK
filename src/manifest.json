{
  "manifest_version": 2,
  "name": "ZTE-MU5001-HACK",
  "version": "3.0.4",
  "description": "Enables hidden menus and metrics for the ZTE-MU5001 router.",
  "permissions": [
    "http://192.168.0.1/*",
    "https://192.168.0.1/*",
    "http://ufi.ztedevice.com/*",
    "https://ufi.ztedevice.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://192.168.0.1/*",
        "https://192.168.0.1/*",
        "http://ufi.ztedevice.com/*",
        "https://ufi.ztedevice.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "chart.bundle.min.js",
    "chart.html",
    "chart.css"
  ]
}
